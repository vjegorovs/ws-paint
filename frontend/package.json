  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "npx webpack serve --mode development",
    "build": "NODE_ENV=production npx webpack --mode production --progress --config webpack.config.prod.js",
    "tsc": "tsc",
    "tscinit": "tsc --init",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:coverage": "jest --coverage",
    "analysis": "npx webpack --analyze --config webpack.config.prod.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo  TODOOO"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/test-setup.js"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "testMatch": [
    "<rootDir>/src/**/*.(test).{js,jsx,ts,tsx}",
    "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
  ],
